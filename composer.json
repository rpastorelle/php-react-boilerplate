{
    "name": "tackk/php-react-boilerplate",
    "description": "A boilerplate for PHP/ReactJS projects",
    "authors": [
        {
            "name": "Pasto",
            "email": "pasto@squadapp.us"
        },
        {
            "name": "DH",
            "email": "danh@squadapp.us"
        }
    ],
    "require": {
        "slim/slim": "^2.6",
        "guzzlehttp/guzzle": "^6.1",
        "vlucas/phpdotenv": "^2.1",
        "slim/views": "^0.1.3",
        "twig/twig": "^1.23"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "App\\": "src/App",
            "Core\\": "src/Core"
        }
    }
}
